<% function renderDisqus(disqus_unique_identifier, disqus_unique_title, disqus_unique_url){ %>
<!--include smthng-->
<!--this is the function that provides meta for each of the required info-->
<% } %>
<% if (disqus_unique_identifier, disqus_unique_title, disqus_unique_url) { %>
<div id="disqus_thread">
</div>

<script type="text/javascript">
    /* * * CONFIGURATION VARIABLES * * */
    var disqus_shortname = 'africanexponent';
    var disqus_identifier = '<%= disqus_unique_identifier%>';
    var disqus_title = '<%= disqus_unique_title%>';
    var disqus_url = '<%= disqus_unique_url%>';

    /* * * DON'T EDIT BELOW THIS LINE * * */
    (function () {
        var dsq = document.createElement('script');
        dsq.type = 'text/javascript';
        dsq.async = true;
        dsq.src = '//' + disqus_shortname + '.disqus.com/embed.js';
        (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
    })();
</script>

<noscript>
    Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript"
                                            rel="nofollow">comments powered by Disqus.</a>
</noscript>
<% } %>